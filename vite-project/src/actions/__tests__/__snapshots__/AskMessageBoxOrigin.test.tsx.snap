// Vitest Snapshot v1

exports[`MatchSnapShot test AskMessageBox 1`] = `
<div
  className="boolean--result"
  data-testid="testAskBox"
>
  <form
    className="form--invitation"
  >
    <label
      className="lbl--invite"
      htmlFor="users"
    >
      Invite
      <img
        alt="no img panel ask invite"
        className="img--invite"
        height="50px"
        width="50px"
      />
      <span
        className="span--invite"
      >
         
      </span>
       to private chat :
    </label>
    <label
      className="lbl--choicechat"
    >
      Would you like 
      <select
        className="select--invite"
        data-testid="select"
        id="invite"
        name="invite"
        onChange={[Function]}
        value="Private"
      >
        <option
          className="option--invite"
          data-testid="select-option"
          name="invite"
          value="Private"
        >
          Private
        </option>
        <option
          className="option--invite"
          data-testid="select-option"
          name="invite"
          value="Info"
        >
          Info
        </option>
        <option
          className="option--invite"
          data-testid="select-option"
          name="invite"
          value="Question"
        >
          Question
        </option>
      </select>
       chat.
    </label>
    <button
      className="btn--invitation"
      data-testid="btnInvite"
      onClick={[Function]}
      type="button"
    >
      Invite
    </button>
    <button
      className="btn--close"
      data-testid="btnClose"
    >
      X
    </button>
  </form>
</div>
`;

